    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Jasmine",
            "program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine.js",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
          },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program with water.svg",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": [
                "--input",
                "${workspaceFolder}/rsc/water.svg"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program with streamlines.svg",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": [
                "--input",
                "${workspaceFolder}/rsc/streamlines.svg"
            ],
        }
    ]
}