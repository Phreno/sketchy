{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Jasmine",
            "program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine.js",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
          },
       {
            "type": "node",
            "request": "launch",
            "name": "sketchy face-outline.svg",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/sketchy.js",
            "args": [
                "--input",
                "${workspaceFolder}/rsc/face-outline.svg"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "scribble squelette-humain.svg",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/scribble.js",
            "args": [
                "--input",
                "${workspaceFolder}/rsc/squelette-humain.svg",
                "-l", "info"
            ],
        }
    ]
}