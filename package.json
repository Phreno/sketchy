  "devDependencies": {
    "http-server": "^14.1.1",
    "jasmine": "^4.2.1",
    "npm-watch": "^0.11.0"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.1.2",
    "commander": "^9.3.0",
    "fast-xml-parser": "^4.0.8",
    "jsdom": "^20.0.0",
    "perfect-freehand": "^1.1.0",
    "point-in-svg-polygon": "^1.0.25",
