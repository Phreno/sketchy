{
  "name": "sketchy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "test": "**/*.js",
    "run_cli": {
      "patterns": [
        "index.js"
      ],
      "extensions": [
        "js"
      ]
    }
  },
  "scripts": {
    "test": "npx jasmine --fail-fast",
